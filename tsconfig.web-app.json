{
    "compileOnSave": false,
    "compilerOptions": {
        "baseUrl": "./",
        "outDir": "./dist/out-tsc",
        "sourceMap": true,
        "declaration": false,
        "module": "es2015",
        "moduleResolution": "node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "target": "es5",
        "typeRoots": [
            "node_modules/@types"
        ],
        "lib": [
            "es2018",
            "dom"
        ],
        "paths": {
            "@wm/build-task": [
                "libraries/build-task/",
                "node_modules/wm/build-task/"
            ],
            "@wm/build-task/*": [
                "libraries/build-task/*",
                "node_modules/wm/build-task/*"
            ],
            "@wm/mobile-build-task": [
                "libraries/mobile-build-task",
                "node_modules/wm/mobile-build-task"
            ],
            "@wm/mobile-build-task/*": [
                "libraries/mobile-build-task/*",
                "node_modules/wm/mobile-build-task/*"
            ],
            "@wm/core": [
                "libraries/core",
                "node_modules/wm/core"
            ],
            "@wm/core/*": [
                "libraries/core/*",
                "node_modules/wm/core/*"
            ],
            "@wm/components": [
                "libraries/components",
                "node_modules/wm/components"
            ],
            "@wm/components/*": [
                "libraries/components/*",
                "node_modules/wm/components/*"
            ],
            "@wm/transpiler": [
                "libraries/transpiler",
                "node_modules/wm/transpiler"
            ],
            "@wm/transpiler/*": [
                "libraries/transpiler/*",
                "node_modules/wm/transpiler/*"
            ],
            "@wm/security": [
                "libraries/security",
                "node_modules/wm/security"
            ],
            "@wm/security/*": [
                "libraries/security/*",
                "node_modules/wm/security/*"
            ],
            "@swipey": [
                "libraries/swipey",
                "node_modules/wm/swipey"
            ],
            "@swipey/*": [
                "libraries/swipey/*",
                "node_modules/wm/swipey/*"
            ],
            "@wm/http": [
                "libraries/http",
                "node_modules/wm/http"
            ],
            "@wm/http/*": [
                "libraries/http/*",
                "node_modules/wm/http/*"
            ],
            "@wm/oAuth": [
                "libraries/oAuth",
                "node_modules/wm/oAuth"
            ],
            "@wm/oAuth/*": [
                "libraries/oAuth/*",
                "node_modules/wm/oAuth/*"
            ],
            "@wm/variables": [
                "libraries/variables",
                "node_modules/wm/variables"
            ],
            "@wm/variables/*": [
                "libraries/variables/*",
                "node_modules/wm/variables/*"
            ],
            "@wm/mobile/core": [
                "libraries/mobile/core",
                "node_modules/wm/mobile/core"
            ],
            "@wm/mobile/core/*": [
                "libraries/mobile/core/*",
                "node_modules/wm/mobile/core/*"
            ],
            "@wm/mobile/components": [
                "libraries/mobile/components",
                "node_modules/wm/mobile/components"
            ],
            "@wm/mobile/components/*": [
                "libraries/mobile/components/*",
                "node_modules/wm/mobile/components/*"
            ],
            "@wm/mobile/offline": [
                "libraries/mobile/offline",
                "node_modules/wm/mobile/offline"
            ],
            "@wm/mobile/offline/*": [
                "libraries/mobile/offline/*",
                "node_modules/wm/mobile/offline/*"
            ],
            "@wm/mobile/variables": [
                "libraries/mobile/variables",
                "node_modules/wm/mobile/variables"
            ],
            "@wm/mobile/variables/*": [
                "libraries/mobile/variables/*",
                "node_modules/wm/mobile/variables/*"
            ],
            "@wm/runtime/base": [
                "libraries/runtime/base",
                "node_modules/wm/runtime/base"
            ],
            "@wm/runtime/base/*": [
                "libraries/runtime/base/*",
                "node_modules/wm/runtime/base/*"
            ],
            "@wm/runtime/dynamic": [
                "libraries/runtime/dynamic",
                "node_modules/wm/runtime/dynamic"
            ],
            "@wm/runtime/dynamic/*": [
                "libraries/runtime/dynamic/*",
                "node_modules/wm/runtime/dynamic/*"
            ],
            "@wm/mobile/runtime": [
                "libraries/mobile/placeholder",
                "node_modules/wm/mobile/placeholder"
            ],
            "@wm/mobile/runtime/*": [
                "libraries/mobile/placeholder/*",
                "node_modules/wm/mobile/placeholder/*"
            ],
            "@wm/mobile/placeholder": [
                "libraries/mobile/placeholder",
                "node_modules/wm/mobile/placeholder"
            ],
            "@wm/mobile/placeholder/*": [
                "libraries/mobile/placeholder/*",
                "node_modules/wm/mobile/placeholder/*"
            ]

{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "angular-app": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/favicon.png",
              "src/assets",
              {
                "glob": "**/*",
                "input": "libraries/locales/moment-timezone",
                "output": "/locales/moment-timezone"
              },
              {
                "glob": "**/*",
                "input": "node_modules/@wavemaker/app-ng-runtime/locales/moment-timezone",
                "output": "/locales/moment-timezone"
              },
              {
                "glob": "**/*",
                "input": "src/servicedefs",
                "output": "/servicedefs/"
              },
              {
                "glob": "**/*",
                "input": "resources",
                "output": "/resources/",
                "ignore": [
                  "**/*.txt"
               ]
              }
            ],
            "styles": [
              "src/styles.css",
              "src/assets/styles/css/wm-style.css",
              "src/assets/styles/css/wm-responsive.css",
              {
                "input": "src/assets/themes/material/style.css",
                "inject": false,
                "bundleName": "wm-styles"
              },
              {
                "input": "src/assets/app.css",
                "inject": false,
                "bundleName": "wm-styles"
              },
              {
                "input": "src/assets/print.css",
                "inject": false,
                "bundleName": "print"
              }
            ],
            "scripts": [
              "./node_modules/d3/dist/d3.min.js",
              "./node_modules/@wavemaker/nvd3/build/nv.d3.min.js",
              "./node_modules/jquery/dist/jquery.min.js",
              "./node_modules/lodash/lodash.min.js",
              "./node_modules/jquery-ui/ui/disable-selection.js",
              "./node_modules/jquery-ui/ui/version.js",
              "./node_modules/jquery-ui/ui/widget.js",
              "./node_modules/jquery-ui/ui/scroll-parent.js",
              "./node_modules/jquery-ui/ui/plugin.js",
              "./node_modules/jquery-ui/ui/data.js",
              "./node_modules/jquery-ui/ui/widgets/mouse.js",
              "./node_modules/jquery-ui/ui/widgets/resizable.js",
              "./node_modules/jquery-ui/ui/widgets/sortable.js",
              "./node_modules/jquery-ui/ui/widgets/draggable.js",
              "./node_modules/jquery-ui/ui/widgets/droppable.js",
              "./libraries/scripts/datatable/datatable.js",
              "./libraries/scripts/swipey/swipey.jquery.plugin.js",
              "./libraries/scripts/jquery.ui.touch-punch/jquery.ui.touch-punch.min.js",
              "./node_modules/summernote/dist/summernote-lite.min.js",
              "./node_modules/iscroll/build/iscroll.js",
              "./node_modules/jssha/dist/sha256.js",
              "./node_modules/@ztree/ztree_v3/js/jquery.ztree.all.js",
              "./libraries/scripts/tree-keyboard-navigation/keyboard-navigation.js"
            ]
          },
          "configurations": {
            "production": {
                "customWebpackConfig": {
                    "path": "./wm-custom-webpack.config.js"
                },
                "indexTransform":"./build-scripts/index-html-transform.js",
                "deployUrl": "ng-bundle/",
                "outputPath": "dist/ng-bundle",
                "index": {
                    "input": "src/index.html",
                    "output": "../../index.html"
                },
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": {
                  "scripts": true,
                  "styles": {
                      "minify": true,
                      "inlineCritical": false
                  },
                  "fonts": true
              },
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb"
                }
              ]
            },
            "development": {
                "customWebpackConfig": {
                    "path": "./wm-custom-webpack.config.js"
                },
                "indexTransform":"./build-scripts/index-html-transform.js",
                "deployUrl": "ng-bundle/",
                "outputPath": "dist/ng-bundle",
                "index": {
                    "input": "src/index.html",
                    "output": "../../index.html"
                },
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.dev.ts"
                }
              ],
              "optimization": false,
              "outputHashing": "none",
              "sourceMap": true,
              "namedChunks": true,
              "aot": true,
              "extractLicenses": false,
                "vendorChunk": true,
              "buildOptimizer": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb"
                }
              ]
            },
            "local": {
                "customWebpackConfig": {
                    "path": "./wm-custom-webpack.config.js"
                },
                "indexTransform":"./build-scripts/index-html-transform.js",
                "deployUrl": "ng-bundle/",
                "outputPath": "dist/ng-bundle",
                "index": {
                    "input": "src/index.html",
                    "output": "../../index.html"
                },
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.dev.ts"
                }
              ],
              "optimization": {
                  "scripts": false,
                  "styles": {
                      "minify": false,
                      "inlineCritical": false
                  },
                  "fonts": true
              },
              "outputHashing": "none",
              "sourceMap": true,
              "namedChunks": true,
              "aot": true,
              "extractLicenses": false,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb"
                }
              ]
            },
            "ng-serve": {
                "indexTransform":"./build-scripts/index-html-transform-ng-serve.ts",
                "deployUrl": "ng-bundle/",
                "outputPath": "dist/ng-bundle",
                "index": "src/index.html",
                  "fileReplacements": [
                      {
                          "replace": "src/environments/environment.ts",
                          "with": "src/environments/environment.dev.ts"
                      }
                  ],
                  "optimization": false,
                  "outputHashing": "none",
                  "sourceMap": true,
                  "namedChunks": true,
                  "aot": true,
                  "extractLicenses": false,
                  "vendorChunk": true,
                  "buildOptimizer": false,
                  "budgets": [
                      {
                          "type": "initial",
                          "maximumWarning": "2mb"
                      }
                  ]
              }
          },
          "defaultConfiguration": "production"
        },
          "serve": {
              "builder": "@angular-builders/custom-webpack:dev-server",
              "configurations": {
                  "development": {
                      "buildTarget": "angular-app:build:ng-serve",
                      "proxyConfig": "proxy.conf.json"
                  }
              },
              "defaultConfiguration": "development"
          },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
              "buildTarget": "angular-app:build"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "src/**/*.ts",
              "src/**/*.html"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "fe5e8f46-2dff-4044-b3b8-8a1307278ce6",
    "schematicCollections": [
      "@angular-eslint/schematics"
    ]
  }
}
